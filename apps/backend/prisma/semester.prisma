model Semester {
    id             String         @id @default(cuid())
    name           String
    createAt       DateTime
    sortOrder      Int            @default(0)
    studentId      String
    student        Student        @relation(fields: [studentId], references: [id], onDelete: Cascade)
    exams          Exam[]
    disciplinaries Disciplinary[]
    classOfficer   String?

    @@map("Semester")
}
